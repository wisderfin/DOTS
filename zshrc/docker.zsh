alias dcs='clear && systemctl start docker'
alias dc='clear && docker-compose stop && docker-compose up --build -d'
alias dca='clear && docker-compose stop && docker-compose up --build'
alias dcp='clear && docker ps'
alias dcd='clear && docker-compose down'
alias dcl='clear && docker-compose logs'
alias docallstop='docker stop $(docker ps -q)'
alias docallrem='docker rm $(docker ps -a -q)'
alias docimgallrem='docker rmi $(docker images -q)'
